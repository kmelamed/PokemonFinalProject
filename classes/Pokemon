public abstract class Pokemon {
	String name;
	String nickName;
	char type;
	int totalHP;
	int currentHP;
	int def;
	int currentDef;
	int speed;
	
	public abstract void fast(Pokemon p);
	public abstract void slow(Pokemon p);
	
	public Pokemon(String n, String nn, char t, int tHP, int d, int s) {
		name = n;
		nickName = nn;
		type = t;
		totalHP = tHP;
		currentHP = totalHP;
		def = d;
		currentDef = def;
		speed = s;
	}
	
}

