import java.util.ArrayList;

public abstract class Character {
	String name;
	ArrayList<Pokemon> pokemon;
	ArrayList<Pokemon> graveyard;
	int row; 
	int col; 

	Character(String n, ArrayList<Pokemon> p, int r, int c) {
		name = n;
		pokemon = p;
		row = r;
		col = c;
		graveyard = new ArrayList<>();
	}
	
}
